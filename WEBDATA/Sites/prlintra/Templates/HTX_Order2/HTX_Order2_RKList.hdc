[AddtionalSQLUseWhere]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条，同时会使用上面的[where]中的sql语句
;示例
Sql1=

[Master]
;显示，多条记录的查询，可在HDT页面中直接使用<HDLoop>显示，并能够提供翻页。and '^^NotFirst^^' = '1'
SQL=select * from v_htx_order v where v.EntGid = __ENTGID__ and v.OpStat = ##OpStat##
Count=select Count(*) from v_htx_order v where v.EntGid = __ENTGID__ and v.OpStat = ##OpStat##

[DefaultWhere]
;多条记录的查询的默认条件
DefaultWhere=

[Where]
;多条记录的查询的条件，可以根据页面传入的变量组合成
1=and lower(v.Num) like '%' || lower(##txt_Num##) || '%'
2=and lower(v.BatchId) like '%' || lower(##txt_BatchId##) || '%'
3=and v.OpDate >= to_date(##txt_BOpDate##, 'yyyy.mm.dd')
4=and v.OpDate <= to_date(##txt_EOpDate##, 'yyyy.mm.dd')+1
5=and (lower(v.FillUsrCode) like '%' || lower(##txt_FillUsrName##) || '%' or lower(v.FillUsrName) like '%' || lower(##txt_FillUsrName##) || '%')
6=and (lower(v.AppCode) like '%' || lower(##txt_AppName##) || '%' or lower(v.AppName) like '%' || lower(##txt_AppName##) || '%')
8=and lower(v.OpType) like '%' || lower(##txt_OpType##) || '%'
[GroupBy]
;多条记录的查询的分组
GroupBy=

[DefaultOrderBy]
;多条记录的查询的默认排序
DefaultOrderBy=v.createdate desc

[OrderBy]
;多条记录的查询的排序,支持多重排序排序的先后顺序由序号决定
1=##selOrderBy##

[Title]
;页面的标题，从Text1～Text30共支持30条

Text1=^^ModuleAtype^^-^^ModuleName^^

;是否翻页：1是，0否
ChangePage=1

;翻页时的每页记录数，如果没有的话，则取HDNet默认值20
ChangePageRow=30

;写入数据库日志
Log=

;页面显示: 0:BodyPage;1:DetailPage;2:BlankPage
PageKind=2

