[Permission]
;判断FlowGid 是否为空
LIMIT1=FlowGid
LIMITDesc1=StringSize[,0]
LIMITRemind1=对不起，您未指定流程信息！请从正确的路径进入

;判断传入FlowGid 是否存在
SQL1=select 1 from WF_Flow wf where wf.EntGid = __ENTGID__ and wf.FlowGid = ##FlowGid##
Remind1=对不起，您指定的流程申请单没有找到<script>alert('对不起，您指定的流程申请单没有找到');window.close()</script>

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出本流程模型信息
SqlForCondition1=select wm.Code ModelCode, wm.Name ModelName,d.Name StepName from wf_Model wm,WF_Task_Define d where wm.EntGid = d.EntGid(+) and wm.EntGid = __ENTGID__ and wm.ModelGid = d.ModelGid(+) and wm.ModelGid = ##ModelGid## and lower(d.code(+)) = replace(lower(##Url##),'save')

;审批备注
SQLForCondition2=select ##CurUsrName##||':'||##AppAssign##||' - 采购：' || OpUsrName || '；日期：' || OpDate || '；供应商：' || OtherName AppFlowMemo from WF_HTX_Order where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 


[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
;更新本流程中主表信息
Sql1=update WF_HTX_Order set (LastUpdDate,Stat) = (select sysdate,'已完成' from dual) where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## 

Sql2=update WF_HTX_Order set (LastUpdDate,AppGid,AppCode,AppName,AppDate) = (select sysdate,__USRGID__,__USRCODE__,__USRNAME__,sysdate from dual) where EntGid = __ENTGID__ AND  FlowGid = ##FlowGid## and##AppAssign## = '同意'

;插入本流程中审批人一信息
SQL3=insert into WF_HTX_Order_App (EntGid,ModelGid,FlowGid,Gid,AppGid,AppCode,AppName,AppType,AppAssign,AppMemo,AppDate) select __ENTGID__,##ModelGid##,##FlowGid##,sys_guid(),__USRGID__,__USRCODE__,__USRNAME__,20,##AppAssign##, ##AppMemo##,sysdate from dual

