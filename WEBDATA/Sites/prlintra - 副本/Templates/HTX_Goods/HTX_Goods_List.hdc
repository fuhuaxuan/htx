[Permission]

[Detail]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条
;取出当前页面的信息
SqlForCondition1=select Name ModuleName,Atype ModuleAtype from rt where lower(id) = lower(##Url##)
SQL1=select Gid UpperGid, Code UpperCode,Name UpperName,length(Code) UpperLen from HTX_Category where EntGid = __ENTGID__ and Code = ##UpperCode##

[AddtionalSQL]
;操作，没有返回的结果，是在数据库中执行，有Insert, Update, Delete命令。从SQL1～SQL20共支持20条
SQL1=

[AddtionalSQLUseWhere]
;显示，单条数据的查询，可以用于HDT显示，有select命令。从SQL1～SQL20共支持20条，同时会使用上面的[where]中的sql语句
;示例
Sql1=

[Master]
;显示，多条记录的查询，可在HDT页面中直接使用<HDLoop>显示，并能够提供翻页。and '^^NotFirst^^' = '1'
SQL=select s.* from v_htx_goods s where s.EntGid = __ENTGID__ and s.UpperCode like ##UpperCode## || '%'
Count=select Count(*) from v_htx_goods s where s.EntGid = __ENTGID__ and s.UpperCode like ##UpperCode## || '%'

[DefaultWhere]
;多条记录的查询的默认条件
DefaultWhere=

[Where]
;多条记录的查询的条件，可以根据页面传入的变量组合成
1=and (lower(s.Code) like '%' || lower(##txt_Name##) || '%' or lower(s.Name) like '%' || lower(##txt_Name##) || '%')
2=and lower(s.Spec) like '%' || lower(##txt_Spec##) || '%'
3=and lower(s.Spec) like '%' || lower(##txt_Spec##) || '%'
4=and lower(s.Unit) like '%' || lower(##txt_Unit##) || '%'
5=and lower(s.Nature) like '%' || lower(##txt_Nature##) || '%'
[GroupBy]
;多条记录的查询的分组
GroupBy=

[DefaultOrderBy]
;多条记录的查询的默认排序
DefaultOrderBy=s.Code

[OrderBy]
;多条记录的查询的排序,支持多重排序排序的先后顺序由序号决定
1=##selOrderBy##

[Title]
;页面的标题，从Text1～Text30共支持30条

Text1=^^ModuleAtype^^-^^ModuleName^^

;是否翻页：1是，0否
ChangePage=1

;翻页时的每页记录数，如果没有的话，则取HDNet默认值20
ChangePageRow=30

;写入数据库日志
Log=

;页面显示: 0:BodyPage;1:DetailPage;2:BlankPage
PageKind=2

[Link]
